<form method="post" action="{{ path('adherent_delete', {'id': adherent.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer cette adherent ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ adherent.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>



